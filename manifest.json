{
  "manifest_version": 2,
  "name": "ClearUrls",
  "version": "0.4",
  "author": "Kevin R., Arne S.",
  "description": "Remove tracking elements form urls.",
  "permissions": [
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["clearurls.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["default.js"]
    }
  ]
}